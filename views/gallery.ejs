<body>
    <div class="gallery-bg-img">
        <header class="">
            <!-- Navbar start -->
            <nav class="navbar navbar-expand-md navbar-light shadow fixed-top">
                <div class="container ps-lg-5 ">
                    <a class="text-35 navbar-brand text-primary" href="/">
                       Yowa Music
                    </a>
                    <button class="navbar-toggler ml-20" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
                    <div class="collapse justify-content-end navbar-collapse" id="navbarNav">
                        <ul class="navbar-nav ">
                            <li class="nav-item text-20">
                                <a class="nav-link" href="/">Home</a>
                            </li>
                            <li class="nav-item text-20">
                                <a class="nav-link" aria-current="page" href="/songs">Songs</a>
                            </li>
                            <li class="nav-item text-20">
                                <a class="nav-link active" href="/gallery">Gallery</a>
                            </li>
                            <li class="nav-item text-20 ms-md-5 ">
                                <a class="nav-link" href="/contact"><input type="button" value="Contact Me" class="btn btn-primary shadow"> </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- Navbar End -->

        </header>
        <div class="container text-center my-auto caption">
            <h1 class="fas fa-camera text-center">
                <%- msg %>
            </h1>
        </div>
    </div>

    <!-- BODY -->
    <div style="background: rgba(186, 59, 108, .8);" class="pb-100">

        <div class="container gallery pt-100">

            <div class="row container mx-auto justify-content-between">

                <img src="images/ig.jpeg" alt="" class="mx-sm-auto-lib shadow mb-5 mb-md-0 left img-fluid  col-sm-10 col-lg-3 col-md-5" srcset="">
                <img src="images/protest2.jpeg" alt="" class="mx-sm-auto-lib center shadow righ col-sm-10 col-lg-4 col-md-5" srcset="">

                <img src="images/protest.jpeg" alt="" class="mx-sm-auto-lib shadow right col-sm-10 col-lg-3 col-md-5 mt-5 mt-lg-0" srcset="">

                <div class="mt-100 mt-sm-0-lib"></div>
                <img src="images/studio img 1.jpeg" alt="" class="left mx-sm-auto-lib shadow  col-sm-10 col-lg-3 col-md-5 mt-5 mt-xxl-0" srcset="">

                <img src="images/millenial.jpg" alt="" class=" mx-sm-auto-lib center shadow  col-sm-10 col-lg-4 col-md-5 mt-5 mt-xxl-0" srcset="">

                <img src="images/on-black.jpg" alt="" class="right mx-sm-auto-lib shadow  col-sm-10 col-lg-3 col-md-5 mt-5 mt-xxl-0" srcset="">

                <div class="mt-100 mt-sm-0-lib"></div>
